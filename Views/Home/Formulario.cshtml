@{
    ViewData["Title"] = "Formulario";
}

<div class="text-center">
    <h1 class="display-4">Formulario</h1>
    <p>Preencha o formulário abaixo.</p>
    <form action="TestFormData" method="post">
        <label for="nome">Nome</label>
        <input type="text" name="nome" id="nome" placeholder="Digite seu nome (exemplo: keke rei delas...)" size="50">
        <label for="email">Email</label>
        <input type="email" name="email" id="email" placeholder="passa seu email pra eu vender pra site de anúncio" size="50">
        <input type="submit" value="Enviar">
    </form>
</div>
